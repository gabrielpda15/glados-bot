{
  "name": "glados-bot",
  "description": "A simple discord bot.",
  "version": "0.0.2",
  "main": "./build/main.js",
  "engines": {
    "node": "18.12.1",
    "npm": "8.19.2",
    "yarn": "1.22.19"
  },
  "scripts": {
    "start": "nodemon --exec \"npx ts-node -r tsconfig-paths/register\" ./src/main.ts",
    "build": "yarn run clean && tsc && yarn run copy-files && yarn run pos-build",
    "clean": "rimraf build/",
    "copy-files": "copyfiles -u 1 -e 'src/**/*{.js,.ts}' src/**/* build/src/",
    "pos-build": "node pos-build.js",
    "typeorm": "node -r ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn run typeorm migration:generate -d typeorm-options.js -p ",
    "migration:run": "yarn run typeorm migration:run -d typeorm-options.js",
    "migration:create": "yarn run typeorm migration:create"
  },
  "repository": "https://github.com/gabrielpda15/glados-bot.git",
  "author": "Gabriel Pupim <gabriel.pda15@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/ffmpeg-static": "^3.0.1",
    "@types/glob": "^8.1.0",
    "@types/node": "^18.15.3",
    "@types/uuid": "^9.0.1",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.21",
    "rimraf": "^4.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.15.0",
    "body-parser": "^1.20.2",
    "discord.js": "^14.8.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.1.0",
    "glob": "^9.3.0",
    "mysql2": "^3.2.0",
    "open": "^8.4.2",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "tsconfig-paths": "^4.1.2",
    "tweetnacl": "^1.0.3",
    "typeorm": "^0.3.13-dev.58fc088",
    "uuid": "^9.0.0",
    "youtube-node": "^1.3.3",
    "ytdl-core": "^4.11.2",
    "ytpl": "^2.3.0"
  }
}
